{
  "name": "@jiggai/recipes",
  "version": "0.2.17",
  "description": "ClawRecipes plugin for OpenClaw (markdown recipes -> scaffold agents/teams)",
  "main": "index.ts",
  "type": "commonjs",
  "openclaw": {
    "extensions": [
      "./index.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.ts",
    "src/",
    "openclaw.plugin.json",
    "recipes/",
    "docs/",
    "clawcipes_cook.jpg",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:all": "npm run test && npm run test:kitchen",
    "test:kitchen": "cd kitchen && npm run test",
    "test:smoke": "node scripts/scaffold-smoke.mjs",
    "smoke": "node scripts/scaffold-smoke.mjs",
    "scaffold:smoke": "node scripts/scaffold-smoke.mjs",
    "kitchen": "cd kitchen && npm run dev",
    "kitchen:prod": "cd kitchen && npm run prod",
    "audit": "npm audit --audit-level=high && cd kitchen && npm audit --audit-level=high",
    "prepare": "husky"
  },
  "keywords": [
    "openclaw",
    "clawrecipes",
    "plugin",
    "recipes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "^0.34.48",
    "json5": "^2.2.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "kitchen/**": "bash -c 'cd kitchen && npm run test'"
  }
}
